<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Respondedores</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/anime.jpg" />
    <icon density="mdpi" src="res/icon/android/anime.jpg" />
    <icon density="hdpi" src="res/icon/android/anime.jpg" />
    <icon density="xhdpi" src="res/icon/android/anime.jpg" />
    <icon density="xxhdpi" src="res/icon/android/anime.jpg" />
    <icon density="xxxhdpi" src="res/icon/android/anime.jpg" />
    <splash density="land-ldpi" src="res/screen/android/anime.jpg" />
    <splash density="land-mdpi" src="res/screen/android/anime.jpg" />
    <splash density="land-hdpi" src="res/screen/android/anime.jpg" />
    <splash density="land-xhdpi" src="res/screen/android/anime.jpg" />
    <splash density="land-xxhdpi" src="res/screen/android/anime.jpg" />
    <splash density="land-xxxhdpi" src="res/screen/android/anime.jpg" />
    <splash density="port-ldpi" src="res/screen/android/anime.jpg" />
    <splash density="port-mdpi" src="res/screen/android/anime.jpg" />
    <splash density="port-hdpi" src="res/screen/android/anime.jpg" />
    <splash density="port-xhdpi" src="res/screen/android/anime.jpg" />
    <splash density="port-xxhdpi" src="res/screen/android/anime.jpg" />
    <splash density="port-xxxhdpi" src="res/screen/android/anime.jpg" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
